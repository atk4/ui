        "ui",
        "web",
        "form",
        "component",
        "widget",
        "render",
        "gadget",
        "button",
        "grid",
        "crud"
    ],
    "homepage": "https://github.com/atk4/ui",
    "version": "dev-develop",
    "license": "MIT",
    "authors": [
        {
            "name": "Romans Malinovskis",
            "email": "romans@agiletoolkit.org",
            "homepage": "https://nearly.guru/"
