{
    "name": "atk4/ui",
    "type": "library",
    "description": "Agile UI - Web Component Framework written in PHP",
    "keywords": ["framework", "ui", "web", "form", "component", "widget", "render", "gadget", "button", "grid", "crud"],
    "homepage": "https://agiletoolkit.org/ui",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Romans Malinovskis",
            "email": "romans@agiletoolkit.org",
            "homepage": "https://nearly.guru/"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "atk4/core": "~1.3.11 || dev-legacy/v1",
        "atk4/data": "~1.4.2. || dev-legacy/v1"
    },
    "require-dev": {
        "phpunit/phpunit": "<6",
        "phpunit/phpcov": "^3.0",
        "codeclimate/php-test-reporter": "*",
        "behat/behat": "^3.4",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "fzaninotto/faker": "*"
    },
    "autoload": {
        "psr-4": {"atk4\\ui\\":"src/"}
    },
    "autoload-dev": {
        "psr-4": {
          "atk4\\ui\\tests\\":"tests/"
        }
    }
}
